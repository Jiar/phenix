{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "arrow-body-style": 0,
    "comma-dangle": [1, "never"],
    "global-strict": 0,
    "linebreak-style": [0, "windows"],
    "semi": ["error", "always"],
    "no-undef": 1,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "no-unreachable": 1,
    "no-alert": 0,
    "one-var": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1
  },
  "globals": {
    "__LOCAL__": false
  }
}
